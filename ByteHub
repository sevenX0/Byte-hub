-- Teleporte para jogadores
local PlayerTP = TeleportTab:Section({ Title = "üë• Teleporte para Jogadores", Icon = "users" })

local function getPlayers()
    local list = {}
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer then
            table.insert(list, player.Name)
        end
    end
    return list
end

local selectedPlayer = ""
local playerDropdown = PlayerTP:Dropdown({
    Title = "Selecionar Jogador",
    Desc = "Escolha um jogador",
    Values = getPlayers(),
    Callback = function(value)
        selectedPlayer = value
    end
})

PlayerTP:Button({
    Title = "üöÄ Teleportar para Jogador",
    Desc = "V√° at√© o jogador selecionado",
    Callback = function()
        if selectedPlayer ~= "" then
            local target = Players:FindFirstChild(selectedPlayer)
            if target and target.Character then
                LocalPlayer.Character:SetPrimaryPartCFrame(target.Character.HumanoidRootPart.CFrame)
                Window:Notify({
                    Title = "Teleporte",
                    Description = "Teleportado para " .. selectedPlayer,
                    Duration = 3
                })
            end
        end
    end
})

PlayerTP:Button({
    Title = "üîÑ Atualizar Lista",
    Desc = "Atualiza lista de jogadores",
    Callback = function()
        playerDropdown:SetValues(getPlayers())
        Window:Notify({
            Title = "Bite Hub",
            Description = "Lista atualizada!",
            Duration = 2
        })
    end
})

