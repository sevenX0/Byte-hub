local AdminTab = Window:Tab({ Title = "üëë Admin", Icon = "shield" })
local AdminSection = AdminTab:Section({ Title = "‚ö° Comandos Admin", Icon = "zap", Opened = true })

local adminTarget = ""
local adminDropdown = AdminSection:Dropdown({
    Title = "Selecionar Alvo",
    Desc = "Escolha quem ser√° afetado",
    Values = getPlayers(),
    Callback = function(value)
        adminTarget = value
    end
})

AdminSection:Button({
    Title = "üíÄ Kill",
    Desc = "Mata o jogador selecionado",
    Callback = function()
        if adminTarget ~= "" then
            local target = Players:FindFirstChild(adminTarget)
            if target and target.Character then
                target.Character:BreakJoints()
                Window:Notify({
                    Title = "Admin",
                    Description = adminTarget .. " foi morto!",
                    Duration = 3
                })
            end
        end
    end
})

AdminSection:Button({
    Title = "üßä Freeze",
    Desc = "Congela o jogador",
    Callback = function()
        if adminTarget ~= "" then
            local target = Players:FindFirstChild(adminTarget)
            if target and target.Character then
                local root = target.Character:FindFirstChild("HumanoidRootPart")
                if root then root.Anchored = true end
                Window:Notify({
                    Title = "Admin",
                    Description = adminTarget .. " congelado!",
                    Duration = 3
                })
            end
        end
    end
})

AdminSection:Button({
    Title = "üî• Unfreeze",
    Desc = "Descongela o jogador",
    Callback = function()
        if adminTarget ~= "" then
            local target = Players:FindFirstChild(adminTarget)
            if target and target.Character then
                local root = target.Character:FindFirstChild("HumanoidRootPart")
                if root then root.Anchored = false end
                Window:Notify({
                    Title = "Admin",
                    Description = adminTarget .. " descongelado!",
                    Duration = 3
                })
            end
        end
    end
})

AdminSection:Button({
    Title = "üîí Jail",
    Desc = "Prende o jogador em uma jaula",
    Callback = function()
        if adminTarget ~= "" then
            local target = Players:FindFirstChild(adminTarget)
            if target and target.Character then
                local root = target.Character:FindFirstChild("HumanoidRootPart")
                if root then
                    -- Cria jaula simples
                    local jail = Instance.new("Part")
                    jail.Size = Vector3.new(10, 10, 10)
                    jail.Anchored = true
                    jail.Transparency = 0.5
                    jail.Color = Color3.fromRGB(255, 100, 0)
                    jail.CFrame = root.CFrame
                    jail.Parent = workspace
                    
                    root.Anchored = true
                    Window:Notify({
                        Title = "Admin",
                        Description = adminTarget .. " preso!",
                        Duration = 3
                    })
                end
            end
        end
    end
})

