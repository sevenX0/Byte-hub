-- Sistema de tags
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local TagSystem = {
    Owner = "ketylnbel",
    SubOwner = "alanzoka2507",
    Tags = {
        ["ketylnbel"] = {
            Text = "[DONO]",
            Color = Color3.fromRGB(255, 50, 50),
            Size = UDim2.new(0, 120, 0, 40)
        },
        ["alanzoka2507"] = {
            Text = "[SUB-DONO]",
            Color = Color3.fromRGB(255, 165, 0),
            Size = UDim2.new(0, 140, 0, 40)
        }
    }
}

-- Fun√ß√£o para aplicar tag
local function applyPlayerTag(player)
    if not player or not player.Character then return end
    
    local function createTag()
        local character = player.Character
        local head = character:WaitForChild("Head", 5)
        if not head then return end
        
        -- Remove tag antiga
        local oldTag = head:FindFirstChild("BiteHubTag")
        if oldTag then oldTag:Destroy() end
        
        -- Verifica se √© dono ou sub-dono
        local tagInfo
        if player.Name == TagSystem.Owner then
            tagInfo = TagSystem.Tags[TagSystem.Owner]
        elseif player.Name == TagSystem.SubOwner then
            tagInfo = TagSystem.Tags[TagSystem.SubOwner]
        else
            -- Tag padr√£o para usu√°rios do hub
            tagInfo = {
                Text = "[BITE HUB USER]",
                Color = Color3.fromRGB(0, 255, 136),
                Size = UDim2.new(0, 150, 0, 40)
            }
        end
        
        -- Cria a tag
        local billboard = Instance.new("BillboardGui")
        billboard.Name = "BiteHubTag"
        billboard.Size = tagInfo.Size
        billboard.StudsOffset = Vector3.new(0, 3.5, 0)
        billboard.AlwaysOnTop = true
        billboard.ResetOnSpawn = false
        billboard.Parent = head
        
        local textLabel = Instance.new("TextLabel")
        textLabel.Size = UDim2.new(1, 0, 1, 0)
        textLabel.BackgroundTransparency = 1
        textLabel.Text = tagInfo.Text
        textLabel.TextColor3 = tagInfo.Color
        textLabel.TextScaled = true
        textLabel.Font = Enum.Font.GothamBold
        textLabel.TextStrokeTransparency = 0
        textLabel.TextStrokeColor3 = Color3.new(0, 0, 0)
        textLabel.Parent = billboard
        
        -- Adiciona brilho para dono
        if player.Name == TagSystem.Owner then
            local glow = Instance.new("UIGradient")
            glow.Color = ColorSequence.new{
                ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 50, 50)),
                ColorSequenceKeypoint.new(0.5, Color3.fromRGB(255, 200, 50)),
                ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 50, 50))
            }
            glow.Rotation = 45
            glow.Parent = textLabel
        end
    end
    
    pcall(createTag)
    player.CharacterAdded:Connect(function()
        task.wait(0.5)
        pcall(createTag)
    end)
end

-- Aplica tags para todos os jogadores
for _, player in pairs(Players:GetPlayers()) do
    applyPlayerTag(player)
end
Players.PlayerAdded:Connect(applyPlayerTag)

