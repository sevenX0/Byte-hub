-- Fun√ß√£o de carregamento
local function updateLoading(percent, text)
    progressBar:TweenSize(UDim2.new(percent/100, 0, 1, 0), "Out", "Linear", 0.2)
    percentText.Text = math.floor(percent) .. "%"
    statusText.Text = text
    task.wait(0.05)
end

-- Simula√ß√£o de carregamento
local loadingSteps = {
    {5, "Verificando executor..."},
    {15, "Carregando bibliotecas..."},
    {30, "Configurando m√≥dulos..."},
    {45, "Inicializando sistemas..."},
    {60, "Preparando interface..."},
    {75, "Otimizando performance..."},
    {85, "Configurando tags..."},
    {95, "Finalizando..."},
    {100, "Pronto! Iniciando Bite Hub..."}
}

for _, step in ipairs(loadingSteps) do
    updateLoading(step[1], step[2])
    task.wait(0.1)
end

task.wait(0.5)
loadingScreen:Destroy()

